<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: lightgrey;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep9.hospital.controller.ProfileManagementFormController">
   <children>
      <TableView layoutX="14.0" layoutY="14.0" prefHeight="314.0" prefWidth="574.0">
        <columns>
          <TableColumn prefWidth="75.0" text="No" />
          <TableColumn prefWidth="75.0" text="Type" />
            <TableColumn prefWidth="75.0" text="Name" />
            <TableColumn prefWidth="75.0" text="Address" />
            <TableColumn prefWidth="150.0" text="Contact details" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox layoutX="45.0" layoutY="343.0" prefHeight="34.0" prefWidth="513.0" spacing="10.0">
         <children>
            <JFXButton fx:id="btnAddProfile" onAction="#btnAddProfile_OnAction" prefHeight="26.0" prefWidth="170.0" style="-fx-background-color: #2F4F4F; -fx-text-fill: white;" text="ADD PROFILE">
               <font>
                  <Font name="Cantarell Bold" size="17.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btnModifyProfile" onAction="#btnModifyProfile_OnAction" prefWidth="170.0" style="-fx-background-color: #2F4F4F; -fx-text-fill: white;" text="MODIFY PROFILE">
               <font>
                  <Font name="Cantarell Bold" size="17.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btnDeleteProfile" onAction="#btnDeleteProfile_OnAction" prefHeight="26.0" prefWidth="170.0" style="-fx-background-color: #2F4F4F; -fx-text-fill: white;" text="DELETE PROFILE">
               <font>
                  <Font name="Cantarell Bold" size="17.0" />
               </font>
            </JFXButton>
         </children>
      </HBox>
   </children>
</AnchorPane>
